	// "editor.suggest.localityBonus": true,
	// "editor.suggest.snippetsPreventQuickSuggestions": true,
	"editor.wordWrapColumn": 120,
	"editor.renderWhitespace": "selection",
  
	"explorer.autoReveal": false,
	"explorer.openEditors.visible": 0,
  
	"extensions.autoUpdate": true,
	"extensions.ignoreRecommendations": false,
  
	"files.autoSave": "onFocusChange",
	"files.eol": "\n",
	"files.exclude": {
	  "**/.DS_Store": true,
	  ".git": true
	},
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
  
	"git.autofetch": true,
	"git.confirmSync": false,
	"githubPullRequests.showInSCM": false,
  
	"gitlens.advanced.messages": {
	  "suppressShowKeyBindingsNotice": true
	},
	"gitlens.views.lineHistory.enabled": false,
	"gitlens.views.repositories.files.layout": "tree",
	"gitlens.views.search.enabled": false,
  
	"problems.decorations.enabled": true,
	"search.exclude": {
	  "**/bower_components": true,
	  "**/node_modules": true,
	  "**/out/**": true,
	  "**/target/**": true,
	  ".build/**": true,
	  ".idea/**": true,
	  ".settings/**": true,
	  "extensions/**/out/**": true,
	  "i18n/**": true,
	  "logs/**": true
	},
  
	"telemetry.enableCrashReporter": false,
	"telemetry.enableTelemetry": false,
  
	"terminal.external.osxExec": "/usr/local/bin/zsh",
	"terminal.integrated.env.osx": {
	  "AUTOSTART_TMUX": "no"
	},
	"terminal.integrated.fontFamily": "Source Code Pro for Powerline",
  
	"typescript.updateImportsOnFileMove.enabled": "never",
  
	"vrdev.experimental.typescript": true,
	"vrdev.tasks.exclude": ["com.vmware.pscoe.library*"],
	"vrdev.views.explorer.actions.layout": "compact",
	"vrdev.buildTools.defaultVersion": "2.5.7",
	"vrdev.log": "debug",
  
	"window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
	"window.zoomLevel": 0,
  
	"workbench.colorCustomizations": {
	  "[Monokai Pro]": {
		"gitDecoration.modifiedResourceForeground": "#fc9867",
		"gitDecoration.untrackedResourceForeground": "#a9dc76",
		"statusBar.debuggingBackground": "#fc9867",
		"statusBar.debuggingForeground": "#4d4c4e"
	  }
	},
	"workbench.editor.closeOnFileDelete": false,
	"workbench.dialogs.useSimplified": true,
	"workbench.settings.editor": "json",
	"workbench.startupEditor": "newUntitledFile",
  
	"workbench.preferredDarkColorTheme": "Monokai Pro",
	"workbench.preferredLightColorTheme": "Github",
	"window.autoDetectColorScheme": true,
  
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  
	"[json]": {
	  "editor.defaultFormatter": "esbenp.prettier-vscode"
	},
  
	"[go]": {
	  "editor.snippetSuggestions": "none",
	  "editor.formatOnSave": true,
	  "editor.codeActionsOnSave": {
		"source.organizeImports": true
	  }
	},
	"go.useLanguageServer": true,
	"go.languageServerExperimentalFeatures": {
	  "diagnostics": true
	},
	"gopls": {
	  "usePlaceholders": true, // add parameter placeholders when completing a function
	  "enhancedHover": true // experimental to improve quality of hover (will be on by default soon)
	},
  
	"prettier.printWidth": 120,
	"prettier.tabWidth": 4,
	"prettier.quoteProps": "consistent",
	"prettier.endOfLine": "lf",
	"explorer.confirmDelete": false,
	"workbench.activityBar.visible": true,
	"yaml.schemas": {
